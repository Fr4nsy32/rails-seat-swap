<div class="container">
  <h1>All Jobs</h1>

  <div class="jobs-container">
    <% @jobs.each do |job| %>
      <% if job.available %>
        <%= link_to job_path(job), class: "job-card" do %>
          <%= render partial: 'shared/job_card', locals: { job: job } %>
        <% end %>
      <% end %>
    <% end %>
  </div>
</div>
