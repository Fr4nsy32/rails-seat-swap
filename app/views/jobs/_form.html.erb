<div class="jobs-form">
  <%= simple_form_for [@user, @job] do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :price %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.input :available_from,
            as: :string,
            input_html: { data: { controller: "datepicker" } } %>
    <%= f.input :credit_to_earn %>
    <%= f.button :submit %>
  <% end %>
</div>
