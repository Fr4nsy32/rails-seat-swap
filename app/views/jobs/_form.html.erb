<div class="jobs-form">
  <%= simple_form_for [@user, @job] do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :price %>
     <%= f.file_field :photo %>
    <%= f.input :available_from %>
    <%= f.input :credit_to_earn %>
    <%= f.button :submit %>
  <% end %>
</div>
